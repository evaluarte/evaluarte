    'Funcion si uso
    Private Function Mediana(ByRef arr() As Double) As Double
        Dim lngElementos As Long, lngMedio As Long
        lngElementos = UBound(arr) - LBound(arr) + 1
        lngMedio = LBound(arr) + (lngElementos \ 2)
        'MsgBox(UBound(arr))
        'MsgBox(LBound(arr))
        'MsgBox(lngElementos)

        If lngElementos And 1 Then
            Mediana = arr(lngMedio)
        Else
            Mediana = (arr(lngMedio) + arr(lngMedio - 1)) / 2
        End If
    End Function

    'Funcion si uso
    Private Sub QuickSort(ByVal lngArray As Object, ByVal lngLB As Long, ByVal lngUB As Long)
        'Dim lngPivote As Long, lngTmp As Long, tmpLB As Long, tmpUB As Long
        'Dim pivote As Integer
        Dim lngPivote As Double, lngTmp As Double, tmpLB As Long, tmpUB As Long
        Dim pivote As Integer

        tmpUB = lngUB
        tmpLB = lngLB

        pivote = (lngLB + lngUB) \ 2
        lngPivote = lngArray(pivote)
        'MsgBox(lngPivote)

        While (tmpLB <= tmpUB)
            'MsgBox(lngArray(tmpLB))
            While (lngArray(tmpLB) > lngPivote And tmpLB < lngUB)
                tmpLB = tmpLB + 1
            End While
            While (lngPivote > lngArray(tmpUB) And tmpUB > lngLB)
                tmpUB = tmpUB - 1
            End While
            If (tmpLB <= tmpUB) Then
                lngTmp = lngArray(tmpLB)
                lngArray(tmpLB) = lngArray(tmpUB)
                lngArray(tmpUB) = lngTmp
                tmpLB = tmpLB + 1
                tmpUB = tmpUB - 1
            End If
        End While
        If (lngLB < tmpUB) Then QuickSort(lngArray, lngLB, tmpUB)
        If (tmpLB < lngUB) Then QuickSort(lngArray, tmpLB, lngUB)
    End Sub

